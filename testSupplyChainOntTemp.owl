    <!-- http://infoneer.txstate.edu/IOF/SC/Act -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SC/Act">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <CommonCoreOntologies:definition>A Process in which at least one Agent plays a causative role.</CommonCoreOntologies:definition>
        <rdfs:label>act</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SC/ActOfShipping -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SC/ActOfShipping">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SC/ActofLocationChange"/>
        <rdfs:label>act of shipping</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SC/ActOfTransportation -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SC/ActOfTransportation">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SC/ActofLocationChange"/>
        <rdfs:label>act of transportation</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SC/ActofAppraisal -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SC/ActofAppraisal">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SC/IntentionalAct"/>
        <CommonCoreOntologies:definition>An Intentional Act that involves evaluating, estimating, or judging the nature, value, importance, condition, or quality of something or someone.</CommonCoreOntologies:definition>
        <CommonCoreOntologies:is_curated_in_ontology>http://www.ontologyrepository.com/CommonCoreOntologies/Mid/EventOntology</CommonCoreOntologies:is_curated_in_ontology>
        <rdfs:label>act of appraisal</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SC/ActofCargoTransportation -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SC/ActofCargoTransportation">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SC/ActOfTransportation"/>
        <CommonCoreOntologies:definition>An Act of Location Change involving the movement of manufactured goods through some Transportation Agent.</CommonCoreOntologies:definition>
        <rdfs:label>act of cargo transportation</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SC/ActofLocationChange -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SC/ActofLocationChange">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SC/ActofMotion"/>
        <CommonCoreOntologies:definition>An Act of Motion in which the location of some Object is changed by some Agent.</CommonCoreOntologies:definition>
        <rdfs:label>act of location change</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SC/ActofMotion -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SC/ActofMotion">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SC/IntentionalAct"/>
        <CommonCoreOntologies:definition>An Intentional Act by which an Agent causes the position or location of some Object to change.</CommonCoreOntologies:definition>
        <rdfs:label>act of motion</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SC/ActofPositionChange -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SC/ActofPositionChange">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SC/ActofMotion"/>
        <CommonCoreOntologies:definition>An Act of Motion in which the position (i.e. the orientation, alignment, or arrangement) of some Object or of one or more of an Object&apos;s parts is changed by some Agent.</CommonCoreOntologies:definition>
        <rdfs:comment>An Act of Position Change does not entail a change of location.</rdfs:comment>
        <rdfs:label>act of position change</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SC/ActofSupplierEvaluation -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SC/ActofSupplierEvaluation">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SC/ActofAppraisal"/>
        <rdfs:label>act of supplier evaluation</rdfs:label>
    </owl:Class>
    


